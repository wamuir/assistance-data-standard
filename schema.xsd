<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2023-1219 Draft">
  <xs:annotation>
    <xs:documentation>Level 1: top level of Assistance Data Standard for assistance, grant, and other transaction instrument documents</xs:documentation>
  </xs:annotation>
  <xs:element name="ADSDocument">
    <xs:annotation>
      <xs:documentation>The root element for Assistance, Grant, and Other Transaction Data Standard  documents.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="SchemaVersionUsed">
          <xs:annotation>
            <xs:documentation>The version of the Assistance, Grant, and Other Transaction Data Standard schema used to produce the document. (see list)</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:token">
              <xs:enumeration value="DRAFT 1.0"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="ADSDocumentForm">
          <xs:annotation>
            <xs:documentation>The standard used to render a copy of the Assistance, Grant, and Other Transaction Data Standard document. </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:choice>
              <xs:element name="StandardInstrumentForm">
                <xs:annotation>
                  <xs:documentation>The Standard Instrument Form. Used for rendering the transaction. (see Standard Instrument Form list at dodprocurementtoolbox.com)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string"/>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AlternateInstrumentForm" type="xs:string">
                <xs:annotation>
                  <xs:documentation>If the form used for rendering is not found in Standard Instrument Form, describe the form here.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:choice>
          </xs:complexType>
        </xs:element>
        <xs:element name="OriginatorDetails">
          <xs:annotation>
            <xs:documentation>Information about the system producing the XML document.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="InternalDocumentNumber" type="xs:string">
                <xs:annotation>
                  <xs:documentation>The internal system identifier of the document by which it is tracked by the system referenced in DoD System or Non DoD System.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:choice>
                <xs:element name="DoDSystem">
                  <xs:annotation>
                    <xs:documentation>Identifies the Department of Defense system or application used to generate the XML document.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element name="DITPRNumber" type="DITPRType">
                        <xs:annotation>
                          <xs:documentation>The DoD Information Technology Portfolio Repository (DITPR) Number assigned to the application or system that generated the XML document.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="SystemAdministratorDoDAAC" type="DoDAACType">
                        <xs:annotation>
                          <xs:documentation>The DoDAAC of the entity that administers the application or system that generated the XML document.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element name="NonDoDSystem">
                  <xs:annotation>
                    <xs:documentation>Identifies the system or application used to generate the XML document when it is not a Department of Defense system or application.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element name="NonDoDEntityIdentifier">
                        <xs:annotation>
                          <xs:documentation>The CAGE, DUNS, Activity Address Code or other identifier of the organization who administers the application or system.  This may or may not be the same entity that uses the application or system.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element name="ActivityAddressCode" minOccurs="0">
                              <xs:annotation>
                                <xs:documentation>The Activity Address Code (AAC).</xs:documentation>
                              </xs:annotation>
                              <xs:simpleType>
                                <xs:restriction base="xs:string">
                                  <xs:pattern value="([0-9AFGJMNRVWY]){1}([0-9A-Z]){5}"/>
                                  <xs:pattern value="([H]){1}([0-9A-HJ-Z]){1}([0-9A-Z]){4}"/>
                                  <xs:pattern value="([S]){1}([0-9A-CE-Z]){1}([0-9A-Z]){4}"/>
                                  <xs:pattern value="([Z]){1}([1-9A-Z]){1}([0-9A-Z]){4}"/>
                                </xs:restriction>
                              </xs:simpleType>
                            </xs:element>
                            <xs:element name="Cage" type="CAGEType" minOccurs="0">
                              <xs:annotation>
                                <xs:documentation>CAGE: Commercial and Government Entity.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="Duns4Number" type="Duns4NumberType" minOccurs="0">
                              <xs:annotation>
                                <xs:documentation>DUNS+4: Data Universal Numbering System identifier with four character suffix.  </xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="DunsNumber" type="DunsNumberType" minOccurs="0">
                              <xs:annotation>
                                <xs:documentation>DUNS:  Data Universal Numbering System Identifier.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="GovernmentUniqueEntityIdentifier" type="GovernmentUniqueEntityIdentiferType" minOccurs="0">
                              <xs:annotation>
                                <xs:documentation>Government Unique Entity Identifier.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="AlternateIdentifier" type="AlternateIdentifierType" minOccurs="0">
                              <xs:annotation>
                                <xs:documentation>Use to provide an identifier that isn't provided as a distinct element.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                      <xs:element name="SystemName">
                        <xs:annotation>
                          <xs:documentation>The name of the application or system.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:minLength value="1"/>
                            <xs:maxLength value="30"/>
                            <xs:whiteSpace value="preserve"/>
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:element>
                      <xs:element name="SystemVersion">
                        <xs:annotation>
                          <xs:documentation>The version of the application or system.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:minLength value="1"/>
                            <xs:maxLength value="10"/>
                            <xs:whiteSpace value="preserve"/>
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:choice>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:choice>
          <xs:element name="AwardInstrument" type="AwardInstrumentType">
            <xs:annotation>
              <xs:documentation>The award or solicitation instrument.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ModificationInstrument" type="ModificationInstrumentType">
            <xs:annotation>
              <xs:documentation>The award modification or solicitation amendment instrument.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Level 2:  DocumentLevel -->
  <xs:complexType name="AwardInstrumentType">
    <xs:annotation>
      <xs:documentation>Award Instrument document</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AwardHeader" type="AwardHeaderType">
        <xs:annotation>
          <xs:documentation>Data common to the entire document.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AwardLineItems" type="AwardLineItemType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>A description of the project or effort that is being identified on this line item.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ModificationInstrumentType">
    <xs:annotation>
      <xs:documentation>Award Modification Instrument.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ModificationHeader" type="ModificationHeaderType">
        <xs:annotation>
          <xs:documentation>Data common to the entire instrument.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ModificationLineItems" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Information about added, changed, and deleted line items.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="AddedLineItems" type="AwardLineItemType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains line items that were added.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeletedLineItems" type="AwardLineItemType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains line items that were removed.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ChangedLineItems" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains line items that were modified.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="LineItemChangeTextDetails" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>Descriptive text of the changes made to the line item.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="LineItemChangeText" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>Description of the line item change that applies to the document.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="Section" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Location within the document structure. Used to know where to render information. (see Section list at dodprocurementtoolbox.com)</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="CurrentLineItem" type="AwardLineItemType">
                    <xs:annotation>
                      <xs:documentation>The line item after the change was applied.  The result.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PreviousLineItem" type="AwardLineItemType">
                    <xs:annotation>
                      <xs:documentation>The line item before the change was applied.  The original.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!-- Level 3 - Complex Types and Elements used within the ADS. -->
  <xs:complexType name="AcceptanceInspectionType">
    <xs:sequence>
      <xs:element name="Action" type="AcceptInspectActionType">
        <xs:annotation>
          <xs:documentation>Acceptance, Inspection or Both (see list)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Location" type="AcceptInspectLocationType">
        <xs:annotation>
          <xs:documentation>Where the action takes place. (see list)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Instructions" type="xs:string">
        <xs:annotation>
          <xs:documentation>Performance, shipment, delivery, and other criteria that the DoD acceptor and/or inspector must verify has been met by the contractor prior to stating that inspection was passed or to accepting the product/service. </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AddressContactType">
    <xs:annotation>
      <xs:documentation>Contains Address and Contact information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Address" type="OrganizationDetailType">
        <xs:annotation>
          <xs:documentation>The address and organization identifiers.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Contact" type="ContactType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>A contact for the organization/address.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AddressDetailsModificationType">
    <xs:annotation>
      <xs:documentation>Award Modification Address structure.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="AddressDescription" type="xs:string">
          <xs:annotation>
            <xs:documentation>Description of the address. (see Address Description list at dodprocurementtoolbox.com)</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="AlternateAddressDescription" type="xs:string">
          <xs:annotation>
            <xs:documentation>Description of the address when not covered by one of the address description options.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="AddressContactModificationDetails" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Records addresses and contacts that have been added, removed, or modified.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="AddedAddress" type="AddressContactType">
              <xs:annotation>
                <xs:documentation>Contains addresses that are added.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeletedAddress" type="AddressContactType">
              <xs:annotation>
                <xs:documentation>Contains addresses that are removed.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ChangedAddress">
              <xs:annotation>
                <xs:documentation>Contains addresses that are modified.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="AddressChangeText">
                    <xs:annotation>
                      <xs:documentation>Description of the change to the address and contacts.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CurrentAddress" type="AddressContactType">
                    <xs:annotation>
                      <xs:documentation>The address and contacts after the changes were applied.  The results.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PreviousAddress" type="AddressContactType">
                    <xs:annotation>
                      <xs:documentation>The address and contacts before the changes were applied.  The original.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="AcceptanceInspection" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Records changes to acceptance and/or inspection instructions when the address denotes an entity performing acceptance, inspection, or both.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="AcceptanceInspectionType">
              <xs:attributeGroup ref="ChangeSection-AcceptanceInspection"/>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="BusinessClassificationModificationDetails" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Records changes to a Recipient’s business classification such as Hospital, Large, Small, etc.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="AddedBusinessClassification" type="BusinessClassificationType">
              <xs:annotation>
                <xs:documentation>Contains the business classification that is added.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeletedBusinessClassification" type="BusinessClassificationType">
              <xs:annotation>
                <xs:documentation>Contains the business classification that is removed.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ChangedBusinessClassification">
              <xs:annotation>
                <xs:documentation>Contains the business classification that is modified.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="BusinessClassificationChangeText">
                    <xs:annotation>
                      <xs:documentation>Description of the change to the business classification.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CurrentBusinessClassification" type="BusinessClassificationType">
                    <xs:annotation>
                      <xs:documentation>The business classification after the changes were applied.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PreviousBusinessClassification" type="BusinessClassificationType">
                    <xs:annotation>
                      <xs:documentation>The business classification before the changes were applied.  The original.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AddressDetailType">
    <xs:annotation>
      <xs:documentation>Award Address structure</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="AddressDescription" type="xs:string">
          <xs:annotation>
            <xs:documentation>Description of the address. (see Address Description list at dodprocurementtoolbox.com)</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="AlternateAddressDescription" type="xs:string">
          <xs:annotation>
            <xs:documentation>Description of the address when not covered by one of the address description options.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="Address" type="OrganizationDetailType">
        <xs:annotation>
          <xs:documentation>The address and organization identifiers.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Contact" type="ContactType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>A contact for the organization/address. This is where you add your technical representative, etc.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AcceptanceInspection" type="AcceptanceInspectionType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Provides acceptance and/or inspection instructions when the address denotes an entity performing acceptance, inspection, or both.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BusinessClassification" type="BusinessClassificationType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Recipient’s business classification such as Hospital, Large, Small, etc.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AlternateIdentifierType">
    <xs:sequence>
      <xs:element name="Description" type="xs:string">
        <xs:annotation>
          <xs:documentation>A brief title or description of the alternate identifier.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Value" type="xs:string">
        <xs:annotation>
          <xs:documentation>The value of the alternate identifier.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AmountType">
    <xs:sequence>
      <xs:element name="AmountDescription" type="xs:string">
        <xs:annotation>
          <xs:documentation>Type of Amount. (see Amount Description list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Amount" type="DecimalElement5ZeroPlusType">
        <xs:annotation>
          <xs:documentation>The amount value.  On an award, must be greater than zero. On a modification, may be zero or greater only when Amount Description is "Header Only - Total Value".</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CancellationCeilingDateAndDescription" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>May only be provided when Amount Description is Cancellation Ceiling.  Required when multiple Cancellation Ceilings are provided.  </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AssistanceListingInformationType">
    <xs:annotation>
      <xs:documentation>Used to provide Asslistance Listing Numbers on Assistnace documents.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AssistanceListingNumber" type="AssistanceListingNumberType">
        <xs:annotation>
          <xs:documentation>The number of the assistance listing. Same as FAADC reporting element CFDA Number.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AssistanceListingTitle" type="xs:string">
        <xs:annotation>
          <xs:documentation>The title corresponding to the assistance listing number.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AttachmentType">
    <xs:annotation>
      <xs:documentation>Attachment structure.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AttachmentDescription">
        <xs:annotation>
          <xs:documentation>A description of the attached document. </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="StandardAttachment" type="xs:string">
              <xs:annotation>
                <xs:documentation>Description of the attachment. (see Standard Attachment list at dodprocurementtoolbox.com)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="OtherAttachment" type="xs:string">
              <xs:annotation>
                <xs:documentation>Description of the attachment when not covered by one of the standard attachment options.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="AttachmentName">
        <xs:annotation>
          <xs:documentation>The name of the attachment including version number if applicable e.g. Scheduled GFP - 4.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:string">
              <xs:attribute name="attachmentSecurityLevel" use="optional">
                <xs:annotation>
                  <xs:documentation>Provide when a government security classification is assigned to the document.  (see list)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:token">
                    <xs:enumeration value="Confidential"/>
                    <xs:enumeration value="Controlled Unclassified Information (CUI)"/>
                    <xs:enumeration value="Secret"/>
                    <xs:enumeration value="Top Secret"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="AttachmentBinaryText" type="xs:base64Binary" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The attachment contents in binary text.   Cannot be provided when Attachment Security Level is populated.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AttachmentDataFormat" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The format of the attachment such as Microsoft Word, Power Point, etc. Required when Attachment Binary Text is provided.  (see Attachment Data Format list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AttachmentDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The date the attached document was created or modified in CCYY-MM-DD format.  It is not the date the document was attached to the instrument.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AttachmentNumber" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>An identifier used within the instrument to reference the attachment.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AttachmentReferenceIdentifier" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A number such as a MIL-STD Number that describes the attachment. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AttachmentURL" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The URL of where the attachment is located on the World Wide Web. </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AwardHeaderType">
    <xs:sequence>
      <xs:element name="InstrumentIdentifier" type="InstrumentIdentifierType" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Unique Identifier for the instrument.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BasicInformation" type="HeaderBasicInformationType">
        <xs:annotation>
          <xs:documentation>Miscellaneous instrument information.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ClauseInformation" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Article, clause or provision information that applies to the non-procurement instrument.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="RegulationURL" type="xs:string" maxOccurs="unbounded">
              <xs:annotation>
