          <xs:documentation>Pricing arrangement type applicable to the instrument line. (see Pricing Arrangement list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SpecialReportingRequirement" type="xs:string" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Denotes special reporting requirements when needed.  (see Special Reporting Requirement list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AssistanceDetails" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Information related to lines on grants and assistance documents.  Required on Assitance instruments.  May not be provided on any other instrument.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="AssistanceListingInformation" type="AssistanceListingInformationType" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Information on the CFDA listing related to the assistance instrument.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="MiscellaneousTextDetails" type="MiscellaneousTextDetailsType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Text relating to the line item that cannot be captured as discrete elements.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OtherTransactionDetails" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Information related to lines on other transaction documents.  Required on Other Transaction instruments.  May not be provided on any other instrument.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="LineItemSolicitationInformation" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Miscellaneous information about solicitations pertaining to the line item.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="LineItemSpecificCondition" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>Solicitation conditions that are applicable to line items. (see list)</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:token">
                        <xs:enumeration value="Alternate Item Substitution Allowed"/>
                        <xs:enumeration value="Do Not Preship"/>
                        <xs:enumeration value="Equal Item Allowed"/>
                        <xs:enumeration value="Must be on Qualified Suppliers List"/>
                        <xs:enumeration value="Must Ship Complete"/>
                        <xs:enumeration value="No Quantity or Unit of Measure Change Allowed"/>
                        <xs:enumeration value="No Substitution Allowed"/>
                        <xs:enumeration value="Restricted to Qualified Products List (QPL) Products"/>
                        <xs:enumeration value="Ship As Soon As Possible"/>
                        <xs:enumeration value="Shipped in Place"/>
                        <xs:enumeration value="Substitute Item Allowed"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="LineItemSolicitationDates" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>Dates that are relevant to a solicitation line item.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="LineItemSolicitationDateDescription">
                          <xs:annotation>
                            <xs:documentation>Categorizes the date relevant to a solicitation line item. (see list)</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:token">
                              <xs:enumeration value="Date of Site or Property Inspection"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                        <xs:element name="LineItemSolicitationDate" type="DateTimeType">
                          <xs:annotation>
                            <xs:documentation>A date (and time) relevant to the solicitation line item.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="AdditionalInformation" type="xs:string" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>If needed, contains additional information about the date on the solicitation.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="PricingDetails" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>The details about the pricing being solicited.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="OtherPricing" minOccurs="0" maxOccurs="unbounded">
                          <xs:annotation>
                            <xs:documentation>Denotes one or more items that should be addressed when pricing.</xs:documentation>
                          </xs:annotation>
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element name="OtherPricingDescription">
                                <xs:annotation>
                                  <xs:documentation>The aspect to be priced.  (see list)</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                  <xs:restriction base="xs:token">
                                    <xs:enumeration value="Lease to Purchase Price"/>
                                    <xs:enumeration value="Catalog Price"/>
                                    <xs:enumeration value="Hourly Labor Rates"/>
                                    <xs:enumeration value="Packaging Protection Level (Preservation) Price"/>
                                    <xs:enumeration value="Packaging Price"/>
                                    <xs:enumeration value="Replacement Price"/>
                                    <xs:enumeration value="Zone Price"/>
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:element>
                              <xs:element name="DeliveryZone" minOccurs="0" maxOccurs="unbounded">
                                <xs:annotation>
                                  <xs:documentation>Indicates the delivery zone that a price is to be provided for.   When Other Pricing Description is "Zone Price", either Delivery Zone or Shipping Postal Code is required; both may be provided.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                  <xs:restriction base="xs:string">
                                    <xs:minLength value="1"/>
                                    <xs:maxLength value="10"/>
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:element>
                              <xs:element name="LaborRatesHoursEstimate" type="TrueType" minOccurs="0">
                                <xs:annotation>
                                  <xs:documentation>Denotes that the offeror is to provide the estimated number of hours associated with a labor rate. May only be provided if Other Pricing Description is Hourly Labor Rates. (see list)</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="PackingLevel" minOccurs="0" maxOccurs="unbounded">
                                <xs:annotation>
                                  <xs:documentation>Indicates the packing level that a price is to be provided for.  May only be provided if Other Pricing Description is Packaging Price.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                  <xs:sequence>
                                    <xs:element name="PackingContainer">
                                      <xs:annotation>
                                        <xs:documentation>The container or material used for the packing.  (see list)</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:token">
                                          <xs:enumeration value="463L Air Pallet"/>
                                          <xs:enumeration value="Bag, burlap or cloth"/>
                                          <xs:enumeration value="Bale"/>
                                          <xs:enumeration value="Barrel"/>
                                          <xs:enumeration value="Basket"/>
                                          <xs:enumeration value="Box"/>
                                          <xs:enumeration value="Bundle"/>
                                          <xs:enumeration value="Cabinet"/>
                                          <xs:enumeration value="Can"/>
                                          <xs:enumeration value="Carboy"/>
                                          <xs:enumeration value="Carton"/>
                                          <xs:enumeration value="Case"/>
                                          <xs:enumeration value="Coil"/>
                                          <xs:enumeration value="CONEX SN 00001-99999"/>
                                          <xs:enumeration value="CONEX SN 100000 - 199999"/>
                                          <xs:enumeration value="CONEX SN 200000 - 299999"/>
                                          <xs:enumeration value="CONEX SN 300000 - 399999"/>
                                          <xs:enumeration value="CONEX SN 400000 - 499999"/>
                                          <xs:enumeration value="CONEX SN 500000 - 599999"/>
                                          <xs:enumeration value="CONEX SN 600000 - 699999"/>
                                          <xs:enumeration value="CONEX SN 700000 - 799999"/>
                                          <xs:enumeration value="CONEX SN 800000 - 899999"/>
                                          <xs:enumeration value="CONEX SN 900000 - 999999"/>
                                          <xs:enumeration value="Container, commercial highway lift"/>
                                          <xs:enumeration value="Container, MAC-ISO, It. wgt. 8x8x20 foot air"/>
                                          <xs:enumeration value="Container, Navy cargo transporter"/>
                                          <xs:enumeration value="Container, Other"/>
                                          <xs:enumeration value="Crate"/>
                                          <xs:enumeration value="Cylinder"/>
                                          <xs:enumeration value="Drum"/>
                                          <xs:enumeration value="Duffel Bag"/>
                                          <xs:enumeration value="Engine Container"/>
                                          <xs:enumeration value="Engine cradle or dolly"/>
                                          <xs:enumeration value="Envelope"/>
                                          <xs:enumeration value="Footlocker (Trunk)"/>
                                          <xs:enumeration value="Hamper"/>
                                          <xs:enumeration value="HHG Containers, wood"/>
                                          <xs:enumeration value="Keg"/>
                                          <xs:enumeration value="Loose, not packed"/>
                                          <xs:enumeration value="MILVAN"/>
                                          <xs:enumeration value="Mixed"/>
                                          <xs:enumeration value="Multiwall container secured to warehouse plt"/>
                                          <xs:enumeration value="Pail"/>
                                          <xs:enumeration value="Palletized unit load, Other"/>
                                          <xs:enumeration value="Piece"/>
                                          <xs:enumeration value="Reel"/>
                                          <xs:enumeration value="Roll"/>
                                          <xs:enumeration value="Roll Off, Roll On (RORO)"/>
                                          <xs:enumeration value="Sack, paper"/>
                                          <xs:enumeration value="SDDC leased/controlled SEAVAN or MILVAN (SDDCVAN)"/>
                                          <xs:enumeration value="SEAVAN"/>
                                          <xs:enumeration value="Sheet"/>
                                          <xs:enumeration value="Skid"/>
                                          <xs:enumeration value="Skid, box"/>
                                          <xs:enumeration value="Spool"/>
                                          <xs:enumeration value="Suitcase"/>
                                          <xs:enumeration value="Tank Car"/>
                                          <xs:enumeration value="Tank Truck"/>
                                          <xs:enumeration value="Truck"/>
                                          <xs:enumeration value="Tub"/>
                                          <xs:enumeration value="Tube"/>
                                          <xs:enumeration value="Unitized"/>
                                          <xs:enumeration value="Van Chassis"/>
                                          <xs:enumeration value="Vehicle"/>
                                          <xs:enumeration value="Vehicle in operating condition"/>
                                          <xs:enumeration value="Wrapped"/>
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:element>
                                    <xs:element name="PackingCapacity" minOccurs="0">
                                      <xs:annotation>
                                        <xs:documentation>To what level the container will be packed.  May only be provided when Packing Container is "MILVAN", "SEAVAN", or "SDDC leased/controlled SEAVAN or MILVAN (SDDCVAN)".  (see list)</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:token">
                                          <xs:enumeration value="Empty"/>
                                          <xs:enumeration value="Less Than Capacity"/>
                                          <xs:enumeration value="To Capacity"/>
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:element>
                                    <xs:element name="Packer" minOccurs="0">
                                      <xs:annotation>
                                        <xs:documentation>Specifies who will pack the container.  Not provided when Packing Capacity is "Empty".  (see list)</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:token">
                                          <xs:enumeration value="Contract Shipment Consolidation Facility"/>
                                          <xs:enumeration value="Military Shipping Activity"/>
                                          <xs:enumeration value="Military Terminal"/>
                                          <xs:enumeration value="Ocean Carrier"/>
                                          <xs:enumeration value="Vendor"/>
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:element>
                                    <xs:element name="AdditionalPacking" minOccurs="0">
                                      <xs:annotation>
                                        <xs:documentation>Use when a second packer is expected.  May only be provided when Packing Capacity is "Less Than Capacity"</xs:documentation>
                                      </xs:annotation>
                                      <xs:complexType>
                                        <xs:choice>
                                          <xs:sequence>
                                            <xs:element name="AdditionalPackingCapacity">
                                              <xs:annotation>
                                                <xs:documentation>To what level the container will be packed by the second packer.  (see list)</xs:documentation>
                                              </xs:annotation>
                                              <xs:simpleType>
                                                <xs:restriction base="xs:token">
                                                  <xs:enumeration value="Completed"/>
                                                  <xs:enumeration value="To Capacity"/>
                                                </xs:restriction>
                                              </xs:simpleType>
                                            </xs:element>
                                            <xs:element name="AdditionalPacker">
                                              <xs:annotation>
                                                <xs:documentation>Specifies who will pack the container.  (see list)</xs:documentation>
                                              </xs:annotation>
                                              <xs:simpleType>
                                                <xs:restriction base="xs:token">
                                                  <xs:enumeration value="Contract Shipment Consolidation Facility"/>
                                                  <xs:enumeration value="Military Shipping Activity"/>
                                                  <xs:enumeration value="Military Terminal"/>
                                                  <xs:enumeration value="Ocean Carrier"/>
                                                  <xs:enumeration value="Vendor"/>
                                                </xs:restriction>
                                              </xs:simpleType>
                                            </xs:element>
                                          </xs:sequence>
                                          <xs:element name="CommingledPacking" type="TrueType">
                                            <xs:annotation>
                                              <xs:documentation>Commingled with commercial cargo in accordance with the SDDC Container Agreement and Rate Guide.  Only used when Packer is "Ocean Carrier".  (see list)</xs:documentation>
                                            </xs:annotation>
                                          </xs:element>
                                        </xs:choice>
                                      </xs:complexType>
                                    </xs:element>
                                  </xs:sequence>
                                </xs:complexType>
                              </xs:element>
                              <xs:element name="ProtectionLevel" minOccurs="0">
                                <xs:annotation>
                                  <xs:documentation>Indicates the packaging protection level (preservation) that a price is to be provided for.  May only be provided if Other Pricing Description is Packaging Protection Level (Preservation) Price.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                  <xs:sequence>
                                    <xs:element name="PreservationMethod">
                                      <xs:annotation>
                                        <xs:documentation>Method and sub-method of preservation (MIL-STD-2073-1D, appendix J).  (see list)</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:token">
                                          <xs:enumeration value="Physical Protection (10)"/>
                                          <xs:enumeration value="Preservative Coating Only (20)"/>
                                          <xs:enumeration value="Waterproof or Waterproof-Greaseproof Protection (30)"/>
                                          <xs:enumeration value="Waterproof Bag, Sealed (31)"/>
                                          <xs:enumeration value="Container, Waterproof Bag, Sealed (32)"/>
                                          <xs:enumeration value="Greaseproof-Waterproof Bag, Sealed (33)"/>
                                          <xs:enumeration value="Watervaporproof Protection (40)"/>
                                          <xs:enumeration value="Watervaporproof Bag, Sealed (41)"/>
                                          <xs:enumeration value="Container, Watervaporproof Bag, Sealed (42)"/>
                                          <xs:enumeration value="Floating Waterproof Bag, Sealed (43)"/>
                                          <xs:enumeration value="Rigid Container (Other Than Metal), Sealed (44)"/>
                                          <xs:enumeration value="Rigid Metal Container, Sealed (45)"/>
                                          <xs:enumeration value="Watervaporproof Protection With Desiccant (50)"/>
                                          <xs:enumeration value="Watervaporproof Bag, Sealed (51)"/>
                                          <xs:enumeration value="Container, Watervaporproof Bag, Sealed (52)"/>
                                          <xs:enumeration value="Floating Waterproof Bag, Sealed (53)"/>
                                          <xs:enumeration value="Rigid Container (Other Than Metal), Sealed (54)"/>
                                          <xs:enumeration value="Rigid Metal Container, Sealed (55)"/>
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:element>
                                    <xs:element name="SpecialPreservationCode" minOccurs="0" maxOccurs="unbounded">
                                      <xs:annotation>
                                        <xs:documentation>Specialized preservation codes (MIL_STD_2073-1D J.4.2.1).</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:token">
                                          <xs:enumeration value="AE"/>
                                          <xs:enumeration value="AH"/>
                                          <xs:enumeration value="AU"/>
                                          <xs:enumeration value="AW"/>
                                          <xs:enumeration value="BC"/>
                                          <xs:enumeration value="BD"/>
                                          <xs:enumeration value="BL"/>
                                          <xs:enumeration value="DB"/>
                                          <xs:enumeration value="DC"/>
                                          <xs:enumeration value="DR"/>
                                          <xs:enumeration value="DW"/>
                                          <xs:enumeration value="EK"/>
                                          <xs:enumeration value="GS"/>
                                          <xs:enumeration value="GX"/>
                                          <xs:enumeration value="HM"/>
                                          <xs:enumeration value="JF"/>
                                          <xs:enumeration value="JM"/>
                                          <xs:enumeration value="KD"/>
                                          <xs:enumeration value="KF"/>
                                          <xs:enumeration value="KG"/>
                                          <xs:enumeration value="KH"/>
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:element>
                                    <xs:element name="SupplementaryOrSpecialRequirements" type="xs:string" minOccurs="0">
                                      <xs:annotation>
                                        <xs:documentation>Supplementary or Special Requirements.  (MIL_STD_2073-1D J.1.4.c)</xs:documentation>
                                      </xs:annotation>
                                    </xs:element>
                                  </xs:sequence>
                                </xs:complexType>
                              </xs:element>
                              <xs:element name="ShippingPostalCode" type="PostalCodeType" minOccurs="0" maxOccurs="unbounded">
                                <xs:annotation>
                                  <xs:documentation>The general shipping area for quoting purposes.   When Other Pricing Description is "Zone Price", either Delivery Zone or Shipping Postal Code is required; both may be provided.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                        <xs:element name="QuantityRangePricing" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Denotes the quantity ranges when prices are being solicited for step ladder pricing.</xs:documentation>
                          </xs:annotation>
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element name="QuantityRange" type="QtyAmountRangeType" maxOccurs="unbounded">
                                <xs:annotation>
                                  <xs:documentation>The quantity range for which a price is being solicited.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:element name="UnitOfMeasure" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>The unit of measure of the quantity range.  (see Unit of Measure Without Dollars list at dodprocurementtoolbox.com)</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="SetAsideDetails" type="SetAsideDetailsType" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Provides set aside percentage and organization type.  If not provided, the solicitation is unrestricted.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="OrderingInformation" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Ordering information pertinent to the line item.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="EstimatedQuantity" type="EstimatedQuantityType" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>The estimated total quantity to be ordered. The actual order quantity may be higher or lower.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="Limits" type="LimitsType" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>Ordering limits on the line item.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="OrderVariation" type="OrderVariationType" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Permitted variation in quantity.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="ProductServicesOrdered" type="ProductServiceQuantityType">
              <xs:annotation>
                <xs:documentation>Details of the product or services ordered.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LineItemDatesType">
    <xs:annotation>
      <xs:documentation>Date structure for a line item.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="LineItemDateDescription">
        <xs:annotation>
          <xs:documentation>Description of the date.  (see list)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="Funding Period"/>
            <xs:enumeration value="Lease"/>
            <xs:enumeration value="Line Item Effective Date"/>
            <xs:enumeration value="Option Exercise Date"/>
            <xs:enumeration value="Ordering Period"/>
            <xs:enumeration value="Period of Performance"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:choice>
        <xs:element name="LineItemDate" type="DateTimeType">
          <xs:annotation>
            <xs:documentation>A specific date (and time if needed).</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="LineItemMonth">
          <xs:annotation>
            <xs:documentation>Date in CCYY-MM format.</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:gYearMonth"/>
          </xs:simpleType>
        </xs:element>
        <xs:element name="LineItemPeriod" type="DatePeriodType">
          <xs:annotation>
            <xs:documentation>Denotes a range of dates.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="FundingPeriodDetails" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Details about the funding period.  May only be provided when Line Item Date Description is "Funding Period".  </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="FundingAmount" type="DecimalElement5ZeroPlusType">
              <xs:annotation>
                <xs:documentation>The amount budgeted for the funding period.  </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="FundingPeriodName" type="xs:string">
              <xs:annotation>
                <xs:documentation>The name of the funding period, such as “Year One” or “Base Year”.  </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LineItemIdentifierType">
    <xs:annotation>
      <xs:documentation>Line item type and numbering.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="LineItemType">
        <xs:annotation>
          <xs:documentation>The type of line item. (see list)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="Line"/>
            <xs:enumeration value="Deliverable Subline"/>
            <xs:enumeration value="Informational Subline"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="LineItemBase">
        <xs:annotation>
          <xs:documentation>The first part of the line item.  Must be alphanumeric.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([0-9]){4}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="LineItemExtension" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Extension added to the line item base.  Must be alphanumeric.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([0]){1}([1-9]){1}"/>
            <xs:pattern value="([1-9]){1}([0-9]){1}"/>
            <xs:pattern value="([A-HJ-NP-Z]){2}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LoaCodeType">
    <xs:sequence>
      <xs:element name="ComponentTitle" type="xs:string">
        <xs:annotation>
          <xs:documentation>Component description.  (see LOA Component list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ComponentValue" type="xs:string">
        <xs:annotation>
          <xs:documentation>The value of the component listed in Component Title.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LocationCoordinatesType">
    <xs:annotation>
      <xs:documentation>The longitude and latitude coordinates of a location.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Latitude" type="DecimalElementCoordinateType">
        <xs:annotation>
          <xs:documentation>The location North or South of the Equator.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Longitude" type="DecimalElementCoordinateType">
        <xs:annotation>
          <xs:documentation>The location East or West of the Prime Meridian.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MarkLabelType">
    <xs:annotation>
      <xs:documentation>Identifies the label that should be contained with the mark</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="MarkValue" type="xs:string">
        <xs:annotation>
          <xs:documentation>The text of the mark.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MarkLabel" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>When provided, this is the label that identifies the marking and is part of the marking.  For example, "Serial Number:", "Manufacturer Part Number -", etc.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MarkRangeType">
    <xs:sequence>
      <xs:element name="RangeStart" type="xs:string">
        <xs:annotation>
          <xs:documentation>Starting number of the sequential range</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RangeEnd" type="xs:string">
        <xs:annotation>
          <xs:documentation>Ending number of the sequential range</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MarkLabel" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>When provided, this is the label that identifies the marking and is part of the marking.  For example, "Serial Number:", "Manufacturer Part Number -", etc.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MiscellaneousTextDetailsType">
    <xs:annotation>
      <xs:documentation>Free form text and the Section of the document it belongs to.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="MiscellaneousText" type="xs:string">
          <xs:annotation>
            <xs:documentation>Unformatted text.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="FormattedMiscellaneousText" type="xs:string">
          <xs:annotation>
            <xs:documentation>Formatted text with html tags.  Contents must be enclosed with CDATA.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="Section" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Location within the document structure. Used to know where to render information. (see Section list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ModificationHeaderType">
    <xs:annotation>
      <xs:documentation>Data common to the entire award modification or solicitation amendment instrument.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="InstrumentIdentifier" type="InstrumentIdentifierType">
        <xs:annotation>
          <xs:documentation>The unique identifier of the modified or amended instrument.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="InstrumentIdentifierModificationDetails" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Records instrument identifiers that have been added, removed, or modified.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="AddedInstrumentIdentifier" type="InstrumentIdentifierType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains instrument identifiers that are added.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeletedInstrumentIdentifier" type="InstrumentIdentifierType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains instrument identifiers that are removed.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ChangedInstrumentIdentifier" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains instrument identifiers that are modified.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="InstrumentIdentifierChangeText" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Description of the change to the instrument identifier.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CurrentInstrumentIdentifier" type="InstrumentIdentifierType">
                    <xs:annotation>
                      <xs:documentation>The instrument identifier after the changes were applied. The results.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PreviousInstrumentIdentifier" type="InstrumentIdentifierType">
                    <xs:annotation>
                      <xs:documentation>The instrument identifier before the changes were applied. The original.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="BasicInformation" type="HeaderBasicInformationModificationType">
        <xs:annotation>
          <xs:documentation>Miscellaneous instrument information.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ClauseInformation" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Article, clause or provision information that applies to the non-procurement instrument.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="RegulationURL" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>'The URL where the text of the articles, clauses or provisions in a Regulation, Supplement, or Issuing Agency can be found.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:string">
                    <xs:attributeGroup ref="ChangeDetails-AddDeleteNoChangeText"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="ClauseModificationDetails" type="ClauseModificationType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Records articles, clauses or provisions that have been added, removed, or modified.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="DeliveryDetails" type="DeliveryDetailsModificationType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Delivery details including dates that impact the entire instrument.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="InstrumentAttachments" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Identifies paperwork that is required to be attached to the instrument.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="AddedAttachments" type="AttachmentType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains attachments that were added.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeletedAttachments" type="AttachmentType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains attachments that were removed.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ChangedAttachments" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains attachments that were modified.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="AttachmentChangeText" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Description of the change to the attachment.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CurrentAttachment" type="AttachmentType">
                    <xs:annotation>
                      <xs:documentation>The attachment after the change was applied.  The result.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PreviousAttachment" type="AttachmentType">
                    <xs:annotation>
                      <xs:documentation>The attachment before the change was applied.  The original.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ModificationDetails">
        <xs:annotation>
          <xs:documentation>Information pertinent to the modification instrument.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="DocumentsBeingModified" maxOccurs="2">
              <xs:annotation>
                <xs:documentation>For an Award Modification, the instrument number(s) of the award or one or both of the ACO odification and PCO Modification to which the modification is being applied.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="DocumentBeingModified" type="InstrumentIdentifierType">
                    <xs:annotation>
                      <xs:documentation>The instrument number whose Instrument Effective Date and/or Instrument Periods are being changed.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="DocumentBeingModifiedSignedDate" type="xs:date">
                    <xs:annotation>
                      <xs:documentation>The signature date of the document being modified in CCYY-MM-DD format.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="ModificationDescription" type="xs:string">
              <xs:annotation>
                <xs:documentation>A short text description of the purpose and content of the modification or amendment.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ModificationReason" type="xs:string" minOccurs="0">
              <xs:annotation>
                <xs:documentation>The reason for the modification based on the requirements of the Federal Procurement Data System (FPDS). (see Modification Reason list at dodprocurementtoolbox.com)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="AdditionalModificationReason" type="xs:string" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Reasons for the modification in addition to the one that would be reported to the Federal Procurement Data System (FPDS).  (see Additional Modification Reasons list at dodprocurementtoolbox.com)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ProjectNumber" type="xs:string" minOccurs="0">
              <xs:annotation>
                <xs:documentation>The project number.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="AmendmentInformation" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Information relevant to an amendment. May not be used for modifications.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="AcknowledgeReceiptCopies" type="xs:positiveInteger" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>The number of copies that is to be completed and signed by a recipient to acknowledge receipt of the amendment document.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="AmendmentDescriptiveText" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Amendment text that does not change the text of the solicitation such as questions and answers.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="OfferReceiptExtended" type="TrueFalseType">
                    <xs:annotation>
                      <xs:documentation>Indicates whether or not the hour and date specified for receipt of Offers is extended.  (see list)</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="ModificationAddresses" type="AddressDetailType" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>For a modification, must provide the addresses of the issuing office and the recipient.  For a solicitation amendment, must provide the address of the issuing office.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ModificationAuthority" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Authorization and recipient signature requirement for the modification.  Required on modifications; not allowed on amendments.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="RecipientSignatureRequired" type="TrueFalseType">
                    <xs:annotation>
                      <xs:documentation>States whether or not the recipient is required to sign the modification document.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:choice>
                    <xs:element name="ChangeOrderAuthority" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>The authority under which the change order is issued.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="AdministrativeChanges" type="TrueType">
                      <xs:annotation>
                        <xs:documentation>Denotes that the modification was made to reflect administrative changes pursuant to the authority of FAR 43.103(b)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="SupplementalAgreementAuthority" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>The authority under which the supplemental agreement is entered.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="OtherAuthority" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>The type of modification and authority if not covered by a Change Order, Supplemental Agreement, or Administrative Changes authority.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:choice>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ModificationInstrumentAddresses" type="AddressDetailsModificationType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Addresses being added, changed, or deleted by the modification. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ModificationInstrumentAmounts" type="InstrumentAmountsModificationType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Amounts and pricing that impact the entire instrument. On Amendments, use Header Currency only.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ModifiedInstrumentDates" type="ModifiedDatesHeaderOnlyType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Changes to dates and date periods applicable to the instrument(s) being modified.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="InstrumentDates" type="DatesModificationHeaderOnlyType">
        <xs:annotation>
          <xs:documentation>Effective Date and signatures applicable to the represented instrument.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RecurringPayments" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Recurring payments of a fixed amount that occur on a specified schedule.  Do not provide on a Solicitation.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="AddedRecurringPayments" type="InstallmentType">
              <xs:annotation>
                <xs:documentation>Contains the recurring payments information that were added.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeletedRecurringPayments" type="InstallmentType">
              <xs:annotation>
                <xs:documentation>Contains recurring payments information that was removed.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ChangedRecurringPayments">
              <xs:annotation>
                <xs:documentation>Contains recurring payments information that was modified.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="RecurringPaymentsChangeText" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Description of the change to the recurring payments.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CurrentRecurringPayments" type="InstallmentType">
                    <xs:annotation>
                      <xs:documentation>The recurring payments after the change was applied.  The result.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PreviousRecurringPayments" type="InstallmentType">
                    <xs:annotation>
                      <xs:documentation>The recurring payments before the change was applied.  The original.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="ReferenceNumber" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Use to provide numbers referencing documents (MIPR, SOW, GBL, Requisition etc.). Numbers (FMS Case, Package, Drawing etc.), Codes (FSC, Project, Weapon System, etc.) and other Reference Values.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="AddedReferenceNumber" type="ReferenceNumberType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains reference numbers that were added.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeletedReferenceNumber" type="ReferenceNumberType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains reference numbers that were removed.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ChangedReferenceNumber" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains reference numbers that were modified.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="ReferenceNumberChangeText" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Description of the change to the reference number.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CurrentReferenceNumber" type="ReferenceNumberType">
                    <xs:annotation>
                      <xs:documentation>The reference number after the change was applied.  The result.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PreviousReferenceNumber" type="ReferenceNumberType">
                    <xs:annotation>
                      <xs:documentation>The reference number before the change was applied.  The original.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ReportingInformation" type="ReportingInformationModificationType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Changes to the reporting requirements including schedule and distribution.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ModifiedDatesHeaderOnlyType">
    <xs:sequence>
      <xs:element name="InstrumentEffectiveDate" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Records changes to the effective date of the instrument being modified. Date in CCYY-MM-DD format. May not be provided on an amendment.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:date">
              <xs:attributeGroup ref="ChangeDetails-Date">
                <xs:annotation>
                  <xs:documentation>For modifications, captures changes to a date element.</xs:documentation>
                </xs:annotation>
              </xs:attributeGroup>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="InstrumentPeriods" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Records changes to a period of time applicable to the instrument being modified.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="DatePeriodDescriptionType">
              <xs:attributeGroup ref="ChangeSection-DatePeriodDescriptionType">
                <xs:annotation>
                  <xs:documentation>Records changes to an Instrument Period section.</xs:documentation>
                </xs:annotation>
              </xs:attributeGroup>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OrderVariationType">
    <xs:choice>
      <xs:element name="Percentage">
        <xs:annotation>
          <xs:documentation>Variation expressed as a percentage.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="PercentUnder" type="DecimalElement5ZeroPlusType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Express 4.25% as 4.25</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="PercentOver" type="DecimalElement5ZeroPlusType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Express 4.25% as 4.25</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Quantity">
        <xs:annotation>
          <xs:documentation>Variation expressed as a discrete quantity.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="QuantityUnder" type="DecimalElement5OverZeroType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>The quantity by which the order can be below the expected quantity.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="QuantityOver" type="DecimalElement5OverZeroType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>The quantity by which the order can be above the expected quantity.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="UnitOfMeasure" type="xs:string">
              <xs:annotation>
                <xs:documentation>The unit of measure.  (see Unit of Measure With Dollars list at dodprocurementtoolbox.com)</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="OrganizationAddressType">
    <xs:choice>
      <xs:element name="FreeFormAddress" type="FreeFormAddressType">
        <xs:annotation>
          <xs:documentation>The address in free form text.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RealPropertyAddress" type="RealPropertyAddressType">
        <xs:annotation>
          <xs:documentation>Address structure for real property as defined by the DoD Business Enterprise Architecture (BEA).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="USPostalAddress" type="USPostalAddressType"/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="OrganizationDetailType">
    <xs:annotation>
      <xs:documentation>Generic Organization Details - Common for supplier, buyer, recipient etc.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="OrganizationID" type="OrganizationIDType">
        <xs:annotation>
          <xs:documentation>Organization Identifier.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OrganizationNameAddress" type="OrganizationNameAddressType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Required if the organization cannot be identified in the "OrganizationId" with an acceptable alternative.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OrganizationIDType">
    <xs:annotation>
      <xs:documentation>Organization ID as pre-defined by various entities. Examples are DUNS, CAGE, DoDAAC etc. In accordance with DoD BTA BEA 4.1 OV-7 ORGANIZATION_IDENTIFIER entity.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Cage" type="CAGEType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>CAGE: Commercial and Government Entity.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CountryCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Three character alpha Country code from Geopolitical Entities, Names, and Codes (GENC).  (see Country list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([A-Z]){3}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="DoDAAC" type="DoDAACType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>DODAAC: Department of Defense Activity Address Code. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Duns4Number" type="Duns4NumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>DUNS+4: Data Universal Numbering System identifier with four character suffix.  </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DunsNumber" type="DunsNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>DUNS:  Data Universal Numbering System Identifier.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EFTIndicator" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The Electronic Funds Transfer (EFT) indicator.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([0-9A-Z]){4}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="EmployerIdentificationNumber" minOccurs="0">
        <xs:annotation>
          <xs:documentation>An Employer Identification Number (EIN) issued by the IRS and used to identify a business entity.  Also known as a federal tax identification number.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([0-9]){9}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="GovernmentUniqueEntityIdentifier" type="GovernmentUniqueEntityIdentiferType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Government Unique Entity Identifier.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Mapac" type="MAPACType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>MAPAC: Military Assistance Program Address Code.  </xs:documentation>
        </xs:annotation>
      </xs:element>
