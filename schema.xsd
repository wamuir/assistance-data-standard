        </xs:annotation>
      </xs:element>
      <xs:element name="ItemDeliveryLeadTime" type="DeliveryLeadTimeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Delivery lead time information.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ItemDeliverySchedule" type="ItemDeliveryScheduleType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Schedule delivery details.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ItemMarks" minOccurs="0" maxOccurs="unbounded">
