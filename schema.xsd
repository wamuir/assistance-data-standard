                <xs:documentation>The item for which a reference number is being provided.  (see Reference Description list at dodprocurementtoolbox.com)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="AlternateDescription" type="xs:string">
              <xs:annotation>
                <xs:documentation>Description of the reference number when not covered by one of the reference description options.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
          <xs:element name="ReferenceValue" type="xs:string">
            <xs:annotation>
              <xs:documentation>The reference number which identifies the item. </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:element name="PurchaseRequestDetails" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation>Provide one iteration for each referenced Purchase Request/MIPR and its corresponding Line item number.  May only be provided at the line item level.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:choice>
                <xs:element name="PurchaseRequisitionNumber" type="xs:string">
                  <xs:annotation>
                    <xs:documentation>The Purchase Requisition Number. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="MIPRNumber" type="xs:string">
                  <xs:annotation>
                    <xs:documentation>The Military Interdepartmental Purchase Request (MIPR) Number</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:choice>
              <xs:element name="PurchaseRequisitionLineItemNumber" type="xs:string">
                <xs:annotation>
                  <xs:documentation>The Purchase Request or Military Interdepartmental Purchase Request (MIPR) Line Item Number.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
      <xs:element name="VersionDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The date that identifies the version of the item or document provided in ReferenceValue.  Date in CCYY-MM-DD format.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
