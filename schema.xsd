  <xs:simpleType name="ACRNType">
    <xs:annotation>
      <xs:documentation>Accounting Classification Reference Number (ACRN).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="2"/>
      <xs:pattern value="([0-9A-HJ-NP-Z]){2}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AgencyAccountingIdentifierType">
    <xs:annotation>
      <xs:documentation>Six digit Agency Accounting Identifier</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9]){5}([1-9]){1}"/>
      <xs:pattern value="([0-9]){4}([1-9]){1}([0-9]){1}"/>
      <xs:pattern value="([0-9]){3}([1-9]){1}([0-9]){2}"/>
      <xs:pattern value="([0-9]){2}([1-9]){1}([0-9]){3}"/>
      <xs:pattern value="([0-9]){1}([1-9]){1}([0-9]){4}"/>
      <xs:pattern value="([1-9]){1}([0-9]){5}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AssistanceListingNumberType">
    <xs:restriction base="xs:string">
      <xs:pattern value="(\d){2}(\.)(\d){3}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CAGEType">
    <xs:annotation>
      <xs:documentation>Commercial and Government Entity (CAGE) code.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="5"/>
      <xs:pattern value="([0-9A-Z]){5}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DecimalElement2OverZeroType">
    <xs:annotation>
      <xs:documentation>Format:  Decimal type with 2 fraction digits and greater than zero.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="2"/>
      <xs:minInclusive value="0.01"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DecimalElement5OverZeroType">
    <xs:annotation>
      <xs:documentation>Format:  Decimal type with 5 fraction digits and greater than zero.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="5"/>
      <xs:minInclusive value="0.00001"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DecimalElement5Type">
    <xs:annotation>
      <xs:documentation>Format:  Decimal type with 5 fraction digits.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="5"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DecimalElement5ZeroPlusType">
    <xs:annotation>
      <xs:documentation>Format:  Decimal type with 5 fraction digits and greater than zero.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="5"/>
      <xs:minInclusive value="0"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DecimalElementCoordinateType">
    <xs:annotation>
      <xs:documentation>Format:  Decimal between -180 and 180 with 5 decimal positions</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="5"/>
      <xs:minInclusive value="-180"/>
      <xs:maxInclusive value="180"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DITPRType">
    <xs:annotation>
      <xs:documentation>The DoD Information Technology Portfolio Repository (DITPR) Number.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="8"/>
      <xs:pattern value="([0-9])*([1-9]){1}([0-9])*"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DoDAACType">
    <xs:annotation>
      <xs:documentation>Department of Defense Activity Address Code (DoDAAC).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="6"/>
      <xs:pattern value="([0-9ACE-HJL-OQRSUVWYZ]){1}([0-9A-Z]){5}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DoDAACType-BadMod">
    <xs:annotation>
      <xs:documentation>Invalid Department of Defense Activity Address Code (DoDAAC).  Used to carry old bad values on Modification/Amendment documents.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="6"/>
      <xs:pattern value="([0-9A-Z]){6}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Duns4NumberType">
    <xs:annotation>
      <xs:documentation>Duns + 4 format.  9 numeric followed by 4 alphanumeric.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9]){9}([0-9A-Z]){4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DunsNumberType">
    <xs:annotation>
      <xs:documentation>Duns format.  9 numeric.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9]){9}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="FEDAACType-NoContractor">
    <xs:annotation>
      <xs:documentation>Federal Activity Address Code (FedAAC), DoD contractor values omitted.  (pos 1-2 = "HG", "SD", and "Z0")</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="6"/>
      <xs:pattern value="([0-9ACEFJL-OQRUVWY]){1}([0-9A-Z]){5}"/>
      <xs:pattern value="H([0-9A-FH-Z]){1}([0-9A-Z]){4}"/>
      <xs:pattern value="S([0-9A-CE-Z]){1}([0-9A-Z]){4}"/>
      <xs:pattern value="Z([1-9A-Z]){1}([0-9A-Z]){4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="GovernmentUniqueEntityIdentiferType">
    <xs:annotation>
      <xs:documentation>Standard format for Government Unique Entity Identifier.  Also known as SAMMI.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([1-9A-Z]){1}([0-9A-Z]){11}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="InstrumentModificationIdentifierType">
    <xs:annotation>
      <xs:documentation>Six digit non-Procurement Instrument Modification Identifier.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([AP]){1}([0-9A-HJ-NP-Z]){2}([0-9]){3}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="LineItemBaseType">
    <xs:annotation>
      <xs:documentation>Four digit Line Item Base.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9]){4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="MAPACType">
    <xs:annotation>
      <xs:documentation>Military Assistance Program Address Code (MAPAC).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="6"/>
      <xs:pattern value="([BDIPKTX]){1}([0-9A-Z]){5}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="NonDoDNumberType">
    <xs:annotation>
      <xs:documentation>Four to fifty character non-DoD procurement instrument number.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="4"/>
      <xs:maxLength value="50"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TrueFalseType">
    <xs:annotation>
      <xs:documentation>Format:  Value must be "true" or "false"</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="false"/>
      <xs:enumeration value="true"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TrueType">
    <xs:annotation>
      <xs:documentation>Format:  Value must be "true".</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="true"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
