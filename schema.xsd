            <xs:enumeration value="Daily"/>
            <xs:enumeration value="Monthly"/>
            <xs:enumeration value="Quarterly"/>
            <xs:enumeration value="Semi-Annually"/>
            <xs:enumeration value="Semi-Monthly"/>
            <xs:enumeration value="Weekly"/>
            <xs:enumeration value="Yearly"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="MaximumPayments" type="xs:positiveInteger" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The maximum number of payments.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RecurringPaymentAmount" type="DecimalElement5OverZeroType">
        <xs:annotation>
          <xs:documentation>The amount of the recurring payment.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PaymentPeriod" type="DateOnlyPeriodType">
        <xs:annotation>
          <xs:documentation>The beginning and end dates for the recurring payments.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="InstrumentAmountsModificationType">
    <xs:annotation>
      <xs:documentation>Award Modifiation amount structure.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="FundingModificationDetails" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Changes to federal and non-federal obligation, project costs, and total award values.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="FundingChangeText" type="xs:string">
              <xs:annotation>
                <xs:documentation>Description of the change to the funding amounts.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="FundingAmountsDeltas" type="FundingAmountsDeltasType" maxOccurs="4">
              <xs:annotation>
                <xs:documentation>The change in the funding amount.  If no change, enter zero.  If the current funding amount is less than the previous funding amount, enter a negative number.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="CurrentFundingAmounts" type="FundingType" minOccurs="4" maxOccurs="4">
              <xs:annotation>
                <xs:documentation>The funding amounts after the change was applied.  The result.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="PreviousFundingAmounts" type="FundingType" minOccurs="4" maxOccurs="4">
              <xs:annotation>
                <xs:documentation>The funding amountst before the change was applied.  The original.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="HeaderCurrency" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The buying and/or payment currencies that apply to an award or award modification.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="CurrencyDetailsType">
              <xs:attributeGroup ref="ChangeSection-Currency"/>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="ObligatedAmountsModificationDetails" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Changes to obligation amounts.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="AddedObligatedAmounts" type="InstrumentObligatedAmountType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains obligated amounts that are added.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeletedObligatedAmounts" type="InstrumentObligatedAmountType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Used when obligated amount is reduced to zero because of the modification.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ChangedObligatedAmounts" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Used to record changes to an existing obligated amount and/or associated information that does not result in a zero obligated amount.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="ObligatedAmountsChangeText" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Description of the change to the obligated amount.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="ObligatedAmountDelta" type="DecimalElement5Type">
                    <xs:annotation>
                      <xs:documentation>The change in the obligated amount.  If no change, enter zero.  If the current obligated amount is less than the previous obligated amount, enter a negative number.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CurrentObligatedAmount" type="InstrumentObligatedAmountType">
                    <xs:annotation>
                      <xs:documentation>The obligated amount after the change was applied.  The result.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PreviousObligatedAmount" type="InstrumentObligatedAmountType">
                    <xs:annotation>
                      <xs:documentation>The obligated amount before the change was applied.  The original.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="OtherAmounts" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Amounts other than obligated amounts.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="AmountType">
              <xs:attributeGroup ref="ChangeSection-OtherAmounts"/>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="InstrumentAmountsType">
    <xs:annotation>
      <xs:documentation>Award amount structure</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Funding" type="FundingType" minOccurs="0" maxOccurs="4">
        <xs:annotation>
          <xs:documentation>Federal and non-federal obligation, project costs, and total award values.  Required on assistance documents as defined under Titles 2 and 32 of the Code Of Federal Regulations (CFR).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="HeaderCurrency" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Currencies that apply to the entire document.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="AwardCurrency" type="CurrencyDetailsType">
              <xs:annotation>
                <xs:documentation>The buying and/or payment currencies that apply to an award or award modification.  Only provide if one or both of the Buying and Payment currencies is not U.S. Dollars.  </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="SolicitationCurrency">
              <xs:annotation>
                <xs:documentation>The currency requirements for a solicitation response.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:choice>
                  <xs:element name="AnyCurrency" type="TrueType">
                    <xs:annotation>
                      <xs:documentation>Denotes that any currency may be used in the response.  (see list)</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="USDollarsOnly" type="TrueType">
                    <xs:annotation>
                      <xs:documentation>Denotes that the response must be in US dollars.  (see list)</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="ForeignCurrency" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>The textual name of the foreign currency that must be used in the response (may include US Dollar from ISO 4217 from the International Organization for Standardization.  (see Currency list at dodprocurementtoolbox.com)</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string"/>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                </xs:choice>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="ObligatedAmounts" type="InstrumentObligatedAmountType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Obligation amount.  Must be greater than zero.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OtherAmounts" type="AmountType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Amounts other than obligated amounts.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="InstrumentDatesType">
    <xs:annotation>
      <xs:documentation>Dates other than delivery, used in award header only.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="InstrumentEffectiveDate" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Effective date of the instrument. Date in CCYY-MM-DD format.  Required on an award.  May not be provided on a solicitation.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="InstrumentPeriods" type="DatePeriodDescriptionType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>A period of time applicable to the dcoument.  Only one instance per Period Description value is allowed unless Period Description is "Funding Period".</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Awardee" type="SigneeType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>The Awardee's name, contact information, and signature date.   May not be provided on a solicitation.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="WarrantedOfficial" type="SigneeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The name, contact information, and signature date of the warranted Government official (agreements officer, grants officer, contracting officer, etc.). The signature date is also known as the Award date.  Required on an award.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="InstrumentIdentifierType">
    <xs:annotation>
      <xs:documentation>Instrument numbers relevant to the document including the instrument number of the document. </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="InstrumentOrigin" type="xs:string">
        <xs:annotation>
          <xs:documentation>Federal/ DoD Agency or Service from where the instrument is originating. (see Instrument Origin list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="InstrumentVehicle" type="xs:string">
        <xs:annotation>
          <xs:documentation>Instrument vehicle like a Coopertive Agreement etc.  (see Instrument Vehicle list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:element name="DoDInstrumentNumber" type="DoDInstrumentNumberType">
          <xs:annotation>
            <xs:documentation>Instrument Number defined following the rules of the applicable DoD agency and/or vehicle.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="FederalInstrumentNumber" type="FederalInstrumentNumberType">
          <xs:annotation>
            <xs:documentation>Instrument Number defined following the rules of the applicable non-DoD agency and/or vehicle.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="NonDoDNumber" type="NonDoDNumberType">
          <xs:annotation>
            <xs:documentation>Use when the instrument is non-Department of Defense and cannot be mapped using the separate fields under Federal Instrument Number.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="InstrumentDescription" type="InstrumentDescriptionType" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Describes the relationship of the instrument identifier listed to the documemt represented in the transaction. (see list)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="InstrumentObligatedAmountType">
    <xs:annotation>
      <xs:documentation>Obligated amount structure at header level.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ObligatedAmount" type="DecimalElement5OverZeroType">
        <xs:annotation>
          <xs:documentation>Obligation amount.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FinancialReference" type="FinancialReferenceType" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>One or more codes or numbers referencing a funding line.  Accounting Classification Reference Number (ACRN) must be provided.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LoaDetails">
        <xs:annotation>
          <xs:documentation>Financial information based on a line of accounting.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="Blob" type="xs:string">
              <xs:annotation>
                <xs:documentation>Use only when the line of accounting cannot be represented by its individual components.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="LOA" type="LoaCodeType" minOccurs="2" maxOccurs="30">
              <xs:annotation>
                <xs:documentation>The Line Of Accounting represented by its individual components.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="SLOA" type="SLOAType" minOccurs="5" maxOccurs="26">
              <xs:annotation>
                <xs:documentation>The DoD Standard Line Of Accounting (SLOA) represented by its individual components.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ItemDeliveryScheduleType">
    <xs:sequence>
      <xs:element name="QuantityToBeDelivered" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The quantity and frequency of the delivery.</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="false">
          <xs:complexContent>
            <xs:extension base="QuantityType">
              <xs:sequence>
                <xs:element name="FrequencyOfDelivery">
                  <xs:annotation>
                    <xs:documentation>How often the delivery should be made.  (see list)</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                    <xs:restriction base="xs:token">
                      <xs:enumeration value="As Required"/>
                      <xs:enumeration value="Daily"/>
                      <xs:enumeration value="Hourly"/>
                      <xs:enumeration value="Monthly"/>
                      <xs:enumeration value="Quarterly"/>
                      <xs:enumeration value="Weekly"/>
                      <xs:enumeration value="Yearly"/>
                    </xs:restriction>
                  </xs:simpleType>
                </xs:element>
              </xs:sequence>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="LengthOfDeliveryPeriod" type="DurationType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The length of the delivery period.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NumberOfExpectedShipments" type="xs:positiveInteger" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Number of shipments expected to complete the deliverable quantity.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ItemObligatedAmountType">
    <xs:annotation>
      <xs:documentation/>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ObligatedAmount" type="DecimalElement5OverZeroType">
        <xs:annotation>
          <xs:documentation>Obligation amount.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CIN" type="xs:string" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Commitment Identification Number. Unique number used to record and track a commitment in the accounting system.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FinancialReference" type="FinancialReferenceType" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>One or more codes or numbers referencing a funding line.  Accounting Classification Reference Number (ACRN) must be provided.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ItemOtherAmountsType">
    <xs:sequence>
      <xs:element name="AmountDescription" type="xs:string">
        <xs:annotation>
          <xs:documentation>Type of Amount. (see Amount Description list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Amount" type="DecimalElement5ZeroPlusType">
