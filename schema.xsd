            </xs:element>
            <xs:element name="RequiredBefore" type="DateTimeType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Date and time by which an offer must be delivered.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="OfferSignature" type="SigneeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The name, title, and signature date of the offeror.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SetAsideDetails" type="SetAsideDetailsType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Provides set aside percentage and organization type.  If not provided, the solicitation is unrestricted.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SizeStandard" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The threshold of employees or amount the offeror must meet to respond to the solicitation.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="AnnualReceipts" type="xs:positiveInteger">
              <xs:annotation>
                <xs:documentation>Total income in whole figures; for example, 6000.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Employees" type="xs:positiveInteger">
              <xs:annotation>
                <xs:documentation>Number of employees.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="SolicitationContact" type="ContactType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Person or entity that is the contact for the solicitation.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SolicitationDates" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Dates that are relevant to a solicitation.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="SolicitationDateDescription" type="xs:string">
              <xs:annotation>
                <xs:documentation>Categorizes the date relevant to a solicitation. (see Solicitation Date Description list at dodprocurementtoolbox.com)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="SolicitationDate" type="DateTimeType">
              <xs:annotation>
                <xs:documentation>A date (and time) relevant to the solicitation.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="AdditionalInformation" type="xs:string" minOccurs="0">
              <xs:annotation>
                <xs:documentation>If needed, contains additional information about the date on the solicitation.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TableOfContentsType">
    <xs:annotation>
      <xs:documentation>Article title and text.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ArticleNumber">
        <xs:annotation>
          <xs:documentation>The number of the article.  </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:positiveInteger"/>
        </xs:simpleType>
      </xs:element>
      <xs:element name="ArticleTitle">
        <xs:annotation>
          <xs:documentation>The textual name of the article.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:string"/>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="SubArticle" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="SubArticleNumber1">
              <xs:annotation>
                <xs:documentation>The number of the subarticle.  [Would be stand alone but would need to match Article Number]</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:decimal">
                  <xs:minInclusive value="1.001"/>
                  <xs:fractionDigits value="3"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="SubArticleNumber2" type="xs:positiveInteger">
              <xs:annotation>
                <xs:documentation>The number of the subarticle.  [Would be concatenated with Artiicle Number.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="SubArticleTitle">
              <xs:annotation>
                <xs:documentation>The textual name of the subarticle.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:string"/>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TextDetailsType">
    <xs:annotation>
      <xs:documentation>choice between excaped text and formatted text.</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="FormattedText" type="xs:string">
        <xs:annotation>
          <xs:documentation>Text with html tags. Contents must be enclosed with CDATA.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Text" type="xs:string">
        <xs:annotation>
          <xs:documentation>Unformatted text.  Special characters must be escaped.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="USPostalAddressType">
    <xs:annotation>
      <xs:documentation>Mailing address based on US Postal Systems Publication 28</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="NonAddressDataLine" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Contains information that is not required by the US Postal Services for distribution and Delivery.  For example, a mailstop code.  When provided, it is the first line of a printed address.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="AttentionLine" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The person or entity within the Recipient to whom the mail should be directed.  In a printed address, it is placed above the Recipient line.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Recipient">
        <xs:annotation>
          <xs:documentation>Person or entity.  In the printed address, it is placed above the Delivery Address Line or, if provided, the Alternate Location line.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="AlternateLocation" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Provided when Delivery Address Line cannot carry the complete address.  For example, 'Apt C'.  In a printed address, it is placed above Delivery Address Line.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="DeliveryAddressLine">
        <xs:annotation>
          <xs:documentation>The address line.  In a printed address, it is placed above the City/State line.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="PostOfficeBox" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The designator that distinguishes a post office box address.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:choice>
        <xs:element name="City">
          <xs:annotation>
            <xs:documentation>The designator that distinguishes one city from another.</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="40"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="MilitaryOrFederalOverseas">
          <xs:annotation>
            <xs:documentation>The designator that distinguishes one Army/Air Force Post Office (APO), Fleet Post Office (FPO), or Diplomat Post Office (DPO) from another.</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:token">
              <xs:pattern value="APO"/>
              <xs:pattern value="DPO"/>
              <xs:pattern value="FPO"/>
              <xs:enumeration value="APO"/>
              <xs:enumeration value="DPO"/>
              <xs:enumeration value="FPO"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:choice>
      <xs:element name="StateOrProvince">
        <xs:annotation>
          <xs:documentation>Designates the State, Province, or Territory within a country.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="PostalCode" type="PostalCodeType">
        <xs:annotation>
          <xs:documentation>The designator that distinguishes one postal zone from another.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Country">
        <xs:annotation>
          <xs:documentation>The textual name of the country from Geopolitical Entities, Names, and Codes (GENC).  (see Country list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
