  <xs:complexType name="ReportingInformationType">
    <xs:annotation>
      <xs:documentation>Used in reporting information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="ReportDescription">
          <xs:annotation>
            <xs:documentation>The type of report that is to be provided.  (see Report Description list at dodprocurementtoolbox.com)</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="AlternateReportDescription" type="xs:string">
          <xs:annotation>
            <xs:documentation>Description of the report when not covered by one of the report description options.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="FinalReportSchedule">
        <xs:annotation>
          <xs:documentation>When the final report must be provided.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="AtContractEnd" type="TrueType">
              <xs:annotation>
                <xs:documentation>Denotes that the final report is due the same date the contract ends.  (see list)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DaysAfterContractEnd" type="xs:positiveInteger">
              <xs:annotation>
                <xs:documentation>The number of days after the instrument ends that the final report must be provided.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="SpecificDate" type="xs:date">
              <xs:annotation>
                <xs:documentation>The specific date that the final report is to be provided in CCYY-MM-DD format.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="InterimReportSchedule" minOccurs="0">
        <xs:annotation>
          <xs:documentation>When and how often the interim reports must be provided.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ReportFrequency">
              <xs:annotation>
                <xs:documentation>How often the report shall be provided.  (see list)</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:token">
                  <xs:enumeration value="Bimonthly"/>
                  <xs:enumeration value="Monthly"/>
                  <xs:enumeration value="Other"/>
                  <xs:enumeration value="Quarterly"/>
                  <xs:enumeration value="Semiannually"/>
                  <xs:enumeration value="Annually"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="ReportDates" minOccurs="0" maxOccurs="3">
              <xs:annotation>
                <xs:documentation>Dates associated with the Report.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="ReportDateDescription">
                    <xs:annotation>
                      <xs:documentation>Description of the provided date such as date of first submission.  (see list)</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:token">
                        <xs:enumeration value="As of date"/>
                        <xs:enumeration value="Date of First Submission"/>
                        <xs:enumeration value="Subsequent Submittal Date"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:choice>
                    <xs:element name="ReportDate" type="xs:date">
                      <xs:annotation>
                        <xs:documentation>Date in CCYY-MM-DD format.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="ReportLeadTime" type="ReportDeliveryLeadTimeType">
                      <xs:annotation>
                        <xs:documentation>Report lead time information.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:choice>
                  <xs:element name="AdditionalInformation" type="xs:string" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Contains relevant information that cannot be provided elsewhere.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="DistributionDetails" type="DistributionDetailsType" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>To whom and how many copies are to be delivered. </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="RoutingTransitType">
