          <xs:documentation>The amount value.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TargetFeePercentIncrease" type="DecimalElement5OverZeroType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Percent increase in the target fee or target profit.  Express 4.25% as 4.25</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TargetFeePercentDecrease" type="DecimalElement5OverZeroType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Percent decrease in the target fee or target profit. Express 4.25% as 4.25</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ItemPartialDeliveryType">
    <xs:annotation>
      <xs:documentation>Partial delivery structure.  Line item only.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PartialDeliveryQuantity" type="QuantityType">
        <xs:annotation>
          <xs:documentation>The quantity and units for a partial delivery.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PartialDeliveryDates" type="DeliveryDatesType">
        <xs:annotation>
          <xs:documentation>Dates, time and period of a partial delivery.</xs:documentation>
        </xs:annotation>
      </xs:element>
