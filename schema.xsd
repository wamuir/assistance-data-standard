                <xs:documentation>The article, clause or provision after the changes were applied.  The results.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="PreviousClause" type="ClauseDetailsType">
              <xs:annotation>
                <xs:documentation>The article, clause or provision before the changes were applied.  The original.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ClauseTextType">
    <xs:annotation>
      <xs:documentation>Actual Text of the referenced clause - could be URL and title reference, full text and/or fill-in text. </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="ClauseFullText" type="xs:string">
          <xs:annotation>
            <xs:documentation>Complete text of the article, article section, clause or provision.</xs:documentation>
          </xs:annotation>
