      <xs:element name="CountryFIPSCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Country code based on FIPS 10-4 as displayed by the General Services Administration (GSA) Worldwide Geographic Location Codes (GLC).</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([0-9A-Z]){3}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="CountryA3Code" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Three character alpha Country code from Geopolitical Entities, Names, and Codes (GENC).  (see Country list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([A-Z]){3}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="CountryN3Code" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Three digit numeric Country code from Geopolitical Entities, Names, and Codes (GENC).  (see Country list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([0-9]){3}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="CountryName" type="xs:string">
        <xs:annotation>
          <xs:documentation>The textual name of the country from Geopolitical Entities, Names, and Codes (GENC).  (see Country list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LocationDirectionsText" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The narrative geographic description of the Location.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="2000"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="PostalZoneCode">
        <xs:annotation>
          <xs:documentation>Postal Code of an address.  See Postcodes from the Universal Postal Union for the correct values.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="10"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
