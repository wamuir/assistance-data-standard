    </xs:annotation>
    <xs:sequence>
      <xs:element name="AddedClauses" type="ClauseDetailsType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Contains articles, clauses or provisions that are added.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DeletedClauses" type="ClauseDetailsType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Contains articles, clauses or provisions that are removed.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ChangedClauses" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Contains articles, clauses or provisions that are modified.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ClauseChangeText" type="xs:string">
