  <xs:complexType name="ClauseDetailsType">
    <xs:sequence>
      <xs:choice>
        <xs:element name="RegulationOrSupplement" type="xs:string">
          <xs:annotation>
            <xs:documentation>Regulation or Supplement name such as FAR, DFARS and AFARS. (see Regulation And Supplement list at dodprocurementtoolbox.com)</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="IssuingActivity" type="FEDAACType-NoContractor">
          <xs:annotation>
            <xs:documentation>The Activity Address Code of the entity who created the article, clause or provision if the source is not found in the Regulation Or Supplement list.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="ClauseCategory" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Broad category that the article, clause, or provision belongs to such as payment or security.  (see Clause Category list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ClauseNumber" type="xs:string">
        <xs:annotation>
          <xs:documentation>The article, clause or provision number.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ClauseTitle" type="xs:string">
        <xs:annotation>
          <xs:documentation>The title of the article, clause or provision.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ClauseEffectiveDate" type="xs:gYearMonth">
        <xs:annotation>
          <xs:documentation>The year and month the article, clause or provision became effective. (Use CCYY-MM format, where CC stands for century.)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ClauseVariation" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Denotes if the article, clause or provision is an alternate or a deviation.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="AlternateOrDeviation">
              <xs:annotation>
                <xs:documentation>Denotes whether the article, clause or provision variation was an alternate or deviation.  (see list)</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:token">
                  <xs:enumeration value="Alternate"/>
                  <xs:enumeration value="Deviation"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="VariationEffectiveDate" type="xs:gYearMonth">
              <xs:annotation>
                <xs:documentation>The year and month the article, clause or provision variation became effective. (Use CCYY-MM format, where CC stands for century.)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="VariationName" type="xs:string" minOccurs="0">
              <xs:annotation>
                <xs:documentation>When applicable, the alternate or deviation name. For example, for "Alternate A", value in Variation Name would be "A".</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ClauseText" type="ClauseTextType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>The complete text or the filled in text of the article, clause, or provision.   May only have multiple when Article Details is provided.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Section" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Location within the document structure. Used to know where to render information. (see Section list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
