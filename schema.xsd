          <xs:documentation>Identifies marks and numbers associated with a ship-to address.</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="false">
          <xs:complexContent>
            <xs:extension base="ContainerMarksNumbers"/>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LimitsType">
