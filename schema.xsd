  <xs:complexType name="ItemReferenceNumberType">
    <xs:annotation>
      <xs:documentation>A generic structure specifying a reference number.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DescriptionText" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Contains additional information when needed. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:sequence>
          <xs:choice>
            <xs:element name="ReferenceDescription" type="xs:string">
